{
  "name": "tailwind-tint-color",
  "version": "0.0.2",
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
    "serve": "live-server example --open=/index.html",
    "watch": "npm run build -- -w",
    "build": "npx @tailwindcss/cli -i example/input.css -o example/tailwind.css"
  },
  "author": "khsily <khsilyqwerty@gmail.com>",
  "homepage": "https://github.com/khsily/tailwind-tint-color",
  "license": "MIT",
  "description": "A Tailwind CSS plugin that adds tint color utilities using CSS filters",
  "keywords": [
    "tailwindcss",
    "plugin",
    "color",
    "tint",
    "css-filters",
    "svg",
    "icons"
  ],
  "files": [
    "src/",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || >= 4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "concurrently": "^9.2.1",
    "live-server": "^1.2.2",
    "tailwindcss": "^4.1.13"
  }
}
